help () {
  cat <<EOF
  emojify - emoji on the command line ðŸ˜±

  USAGE: emojify [-h|--help] TEXT
EOF
}

# Accept input from $* or from the pipeline
if test "$*" != ''; then
  if [ "$1" == '-h' ] || [ "$1" == '--help' ]; then
    help
    exit 0
  else
    IFS=' '; read -a words_arr <<< $*
    for word in "${words_arr[@]}"; do
      data+=$(emojify $word)
      data+=' '
    done
    echo $data
  fi
else
  while IFS=''; read -r line || [ -n "$line" ]; do
    echo $(emojify $line)
  done
fi
